<!DOCTYPE html>
<html lang="{{.Locale}}">
<head>
  <meta charset="utf-8">
  <title><%= htmlPlugin.options.title %></title>
  <link rel="icon" type="image/png" href="/assets/favicon-32x32.png" sizes="32x32">
  <link rel="icon" type="image/png" href="/assets/favicon-16x16.png" sizes="16x16">
  <meta name="theme-color" content="#ffffff">
  <meta name="color-scheme" content="light dark" />
  <meta name="viewport" content="width=device-width, height=device-height, initial-scale=1, viewport-fit=cover">
  <link rel="stylesheet" type="text/css" href="//{{.Domain}}/assets/fonts/fonts.css">
  <% htmlPlugin.files.css.forEach(function(file) { %>
    <link rel="stylesheet" href="<%- file %>" />
  <% }); %>
  {{.ThemeCSS}}
</head>
<div
  role="application"
  data-cozy="{{.CozyData}}"
>
</div>

<% htmlPlugin.files.js.forEach(function(file) { %>
  <script defer src="<%- file %>"></script>
<% }); %>

